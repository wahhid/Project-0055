        <template id="warehouse_opname_trans_batch">
            <html>
                <head>
                    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
                    <title>Stock System - Ranch Market</title>
                </head>
                <body>
                    <form method="POST" action="/warehouse/opname/trans/qty">
                        <table border="1">
                            <tr>
                                <td>Site</td>
                                <td>
                                    <t t-esc="request.session.warehousename"/>
                                </td>
                            </tr>
                            <tr>
                                <td>Periode</td>
                                <td>
                                    <t t-esc="request.session.periodename"/>
                                </td>
                            </tr>
                            <tr>
                                <td>Bin</td>
                                <td>
                                    <t t-esc="request.session.gondolaname"/>
                                </td>
                            </tr>
                            <tr>
                            <td>Progress</td>
                                <td>
                                    <font color="red">
                                        <t t-if="request.session.step == '1'">
                                            <span>First Collection</span>
                                        </t>
                                        <t t-if="request.session.step == '2'">
                                            <span>Second Collection</span>
                                        </t>
                                        <t t-if="request.session.step == '3'">
                                            <span>Third Collection</span>
                                        </t>
                                    </font>
                                </td>
                            </tr>
                            <tr>
                                <td>Last</td>
                                <td><t t-esc="request.session.lastproduct"/></td>
                            </tr>
                            <tr>
                                <td>Ean</td>
                                <td><t t-esc="request.session.ean"/></td>
                            </tr>
                            <tr>
                                <td>Article</td>
                                <td><t t-esc="request.session.articleid"/></td>
                            </tr>
                             <tr>
                                <td>Product</td>
                                <td><t t-esc="request.session.productname"/></td>
                            </tr>
                            <tr>
                                <td>Batch</td>
                                <td>
                                    <t t-if="request.session.is_batchnos">
                                        <select name="batchno">
                                            <t t-foreach="request.session.batchnos" t-as="batchno">
                                                <option t-att-value="batchno['batchno']"><t t-esc="batchno['batchno']"/></option>
                                            </t>
                                        </select>
                                    </t>
                                    <t t-if="request.session.is_batchnos">
                                        <span>No Batch</span>
                                    </t>
                                </td>
                            </tr>
                            <tr t-if="msg">
                                <td></td>
                                <td><font color="red"><t t-esc="msg"/></font></td>
                            </tr>
                            <t t-if="request.session.is_batchnos">
                                <tr>
                                    <td></td>
                                    <td><input type="submit" value="Submit"/></td>
                                </tr>
                            </t>
                            <tr>
                                <td></td>
                                <td><a href="/warehouse/opname/trans/batchother">Other Batch</a></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td><a href="/warehouse/opname/trans/close">Close Transaction</a></td>
                            </tr>
                            <tr>
                                <td></td>
                                <td></td>
                            </tr>
                        </table>
                    </form>
                </body>
            </html>
        </template>

